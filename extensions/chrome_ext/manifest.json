{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "description": "A simple Chrome extension to get started",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "My Extension"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
